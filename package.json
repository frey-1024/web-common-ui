{
  "name": "web-common-ui",
  "version": "1.0.0",
  "description": "`npm run dev`",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "dev": "start-storybook -p 8081",
    "build": "build-storybook -o build",
    "lint": "eslint --ext .js --ext .ts --ext .tsx src stories --max-warnings 0",
    "start": "npm run dev",
    "test": "jest --config jest.config.js --no-cache",
    "coverage": "jest --config jest.config.js --no-cache --coverage",
    "lib": "npm run lint && npm run build:lib",
    "build:lib": "./node_modules/.bin/gulp build"
  },
  "directories": {
    "test": "tests"
  },
  "files": [
    "lib",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangKunUserGit/web-common-ui.git"
  },
  "author": "zhangkun <1766597067@qq.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zhangKunUserGit/web-common-ui/issues"
  },
  "homepage": "https://github.com/zhangKunUserGit/web-common-ui#readme",
  "peerDependencies": {
    "react": ">=16.6.0",
    "react-dom": ">=16.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "cross-env": "^7.0.2",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.11.0",
    "rollup-plugin-terser": "^5.3.0",
    "semantic-release": "^17.0.6",
    "node-sass": "4.14.1",
    "typescript": "3.8.3"
  },
  "sideEffects": false,
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
